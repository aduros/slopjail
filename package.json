{
  "name": "slopjail",
  "version": "1.0.1",
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    "import": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.mjs"
    },
    "require": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.cjs"
    }
  },
  "keywords": [
    "sandbox",
    "jail",
    "iframe",
    "web-worker",
    "security",
    "isolation",
    "untrusted-code",
    "csp",
    "javascript-sandbox",
    "browser"
  ],
  "files": [
    "./dist"
  ],
  "scripts": {
    "prepare": "lefthook install",
    "build": "vite build",
    "prepack": "npm run build -- --emptyOutDir",
    "release": "bump --commit --tag --push",
    "start": "vite build --watch",
    "test": "vitest run",
    "test:lint": "biome check --diagnostic-level=error",
    "test:types": "tsc --noEmit"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.3.13",
    "@vitest/browser-playwright": "^4.0.18",
    "lefthook": "^2.0.16",
    "typescript": "^5.9.3",
    "version-bump-prompt": "^6.1.0",
    "vite": "^7.3.1",
    "vite-plugin-dts": "^4.5.4",
    "vitest": "^4.0.18"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/aduros/slopjail"
  },
  "dependencies": {
    "shrimp-rpc": "^1.0.1"
  }
}
